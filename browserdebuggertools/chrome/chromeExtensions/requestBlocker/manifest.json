{
  "manifest_version": 3,
  "name": "Request Blocker",
  "description": "Request Blocker",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "windows"
  ],
   "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "requestBlocker.js"
  }
}
